{
    "type": "application",
    "description": "下载curseforge的整合包的php工具",
    "keywords": [
        "php",
        "curseforge"
    ],
    "homepage": "https://github.com/snje1987/cursedown",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Yang Ming",
            "email": "yangming0116@163.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "minifw/console": "^v1.0"
    },
    "autoload": {
        "psr-4": {
            "Org\\Snje\\Cursedown\\": "src/"
        }
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.5"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "scripts": {
        "build": "./vendor-bin/box/vendor/bin/box compile"
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true
        }
    }
}
